      "parameters" : [
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "A unique name used to identify the context being created.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "flowName"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "The duration of accelerometer data that should be used for the current context, expressed in seconds. This value has to be within 2 and 7 seconds (inclusive). We recommend setting this value to 3, which is the default value.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "duration"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "An array of custom signals to be added to the context.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "customSignals"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "The callback that will be executed once the context is ready. If ContextSDK was already running for the duration provided, this callback will be executed immediately. This callback will be executed asynchronously on the main thread.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "callback"
        }
      ]
    },
    {
      "content" : [
        {
          "anchor" : "discussion",
          "level" : 2,
          "text" : "Discussion",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "ContextSDK will automatically figure out if a context object can be generated immediately (in that case, your callback is instantly executed), or if it needs to run for a little while, until itâ€™s ready to execute your callback.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        }
      ],
      "kind" : "content"
