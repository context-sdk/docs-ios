      "kind" : "parameters",
      "parameters" : [
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "A unique name used to identify the context being created.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "flowName"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "The duration of accelerometer data that should be used for the current context, expressed in seconds. This value has to be within 2 and 7 seconds (inclusive). We recommend setting this value to 3, which is the default value.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "duration"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "An array of custom signals to be added to the context.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "customSignals"
        }
      ]
    },
    {
      "content" : [
        {
          "anchor" : "return-value",
          "level" : 2,
          "text" : "Return Value",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "A new context containing signals captured at the moment of calling this method.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        }
      ],
      "kind" : "content"
    },
    {
      "content" : [
        {
          "anchor" : "discussion",
          "level" : 2,
          "text" : "Discussion",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Instant contexts might not include all information if the app wasn’t given enough time prior to calling this method to collect signals. Be sure that the majority of the times when calling this method, the SDK has already had enough time to reach the duration you’ve set.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "Once you’ve decided on a specific ",
                  "type" : "text"
                },
                {
                  "code" : "duration",
                  "type" : "codeVoice"
                },
                {
                  "text" : ", you need to stick with it, as your custom model will be trained and provided for the specific ",
                  "type" : "text"
                },
                {
                  "code" : "duration",
                  "type" : "codeVoice"
                },
                {
                  "text" : " you’ve chosen. If you change the ",
                  "type" : "text"
                },
                {
                  "code" : "duration",
                  "type" : "codeVoice"
                },
                {
                  "text" : " later, you’ll need to re-train your custom model.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "Important",
          "style" : "important",
          "type" : "aside"
        }
      ],
      "kind" : "content"
    }
