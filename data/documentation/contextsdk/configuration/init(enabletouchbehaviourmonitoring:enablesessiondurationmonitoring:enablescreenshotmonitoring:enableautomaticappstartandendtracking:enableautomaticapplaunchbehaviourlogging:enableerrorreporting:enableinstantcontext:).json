    {
      "kind" : "parameters",
      "parameters" : [
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "Determines whether touch behaviour monitoring is enabled. This uses a ",
                  "type" : "text"
                },
                {
                  "code" : "UITapGestureRecognizer",
                  "type" : "codeVoice"
                },
                {
                  "text" : " that always immediately returns ",
                  "type" : "text"
                },
                {
                  "code" : "false",
                  "type" : "codeVoice"
                },
                {
                  "text" : ", ensuring no impact on your app. Only the total count of touches is sent, without any interaction details. Defaults to ",
                  "type" : "text"
                },
                {
                  "code" : "true",
                  "type" : "codeVoice"
                },
                {
                  "text" : ".",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "enableTouchBehaviourMonitoring"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "Logs the duration of the current session in seconds. Defaults to ",
                  "type" : "text"
                },
                {
                  "code" : "true",
                  "type" : "codeVoice"
                },
                {
                  "text" : ".",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "enableSessionDurationMonitoring"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "Tracks the number of screenshots taken during the current session. Defaults to ",
                  "type" : "text"
                },
                {
                  "code" : "true",
                  "type" : "codeVoice"
                },
                {
                  "text" : ".",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "enableScreenshotMonitoring"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "Sends a single ContextSDK event when your app launches and when it transitions to the background. This enables deeper insights into contexts that positively influence session durations and upsells. Defaults to ",
                  "type" : "text"
                },
                {
                  "code" : "true",
                  "type" : "codeVoice"
                },
                {
                  "text" : ".",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "enableAutomaticAppStartAndEndTracking"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "Logs how often and when users launch the app. Metrics are stored locally using ",
                  "type" : "text"
                },
                {
                  "code" : "NSUserDefaults",
                  "type" : "codeVoice"
                },
                {
                  "text" : ", and only aggregated data (e.g., daily app launch counts) is used. Defaults to ",
                  "type" : "text"
                },
                {
                  "code" : "true",
                  "type" : "codeVoice"
                },
                {
                  "text" : ".",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "enableAutomaticAppLaunchBehaviourLogging"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "Enables reporting of unexpected errors back to ContextSDK. These reports help ensure smooth rollouts of new Machine Learning Models over the air. If issues arise while parsing CoreML models, errors are reported while the system falls back to a default model for uninterrupted operation. Defaults to ",
                  "type" : "text"
                },
                {
                  "code" : "true",
                  "type" : "codeVoice"
                },
                {
                  "text" : ".",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "enableErrorReporting"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "Enables instant context support. When ",
                  "type" : "text"
                },
                {
                  "code" : "true",
                  "type" : "codeVoice"
                },
                {
                  "text" : ", ContextSDK allows decisions to be made shortly after launch, using approximately 2% of a single CPU core continuously (e.g., ~0.33% total CPU usage on a 6-core device). Defaults to ",
                  "type" : "text"
                },
                {
                  "code" : "true",
                  "type" : "codeVoice"
                },
                {
                  "text" : ".",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "enableInstantContext"
        }
      ]
    }
  ],
